<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Application_Date_Is_Mandatory</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISNULL(Application_Date__c), NOT(ISBLANK(Application_No__c)))</errorConditionFormula>
    <errorDisplayField>Application_Date__c</errorDisplayField>
    <errorMessage>Application Date is Mandatory</errorMessage>
</ValidationRule>
