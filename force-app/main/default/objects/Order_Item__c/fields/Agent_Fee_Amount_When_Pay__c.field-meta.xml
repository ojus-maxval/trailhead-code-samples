<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Agent_Fee_Amount_When_Pay__c</fullName>
    <externalId>false</externalId>
    <formula>if 
(
(TEXT(Agent_Fee_Currency__c)==TEXT(Billing_Currency__c))
, Agent_Fee__c 
, ((Agent_Fee__c/Agent_Fee_Unit_Rate_When_Pay__c)/Billing_Currency_Unit_Rate_When_Pay__c)
+ (((Agent_Fee__c/Agent_Fee_Unit_Rate_When_Pay__c)/Billing_Currency_Unit_Rate_When_Pay__c)*(CE_Markup_Percentage__c /100))
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Agent Fee Amount (When Pay)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
